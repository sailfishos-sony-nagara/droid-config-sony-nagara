# As we cannot use regex, just a joint part of a string for PDX223 and 224
CPUCHECK_STRING="Sony Corporation. PDX-22"

# lrwxrwxrwx    1 root     root            16 Jun 22 13:03 boot_a -> /dev/block/sda68
# lrwxrwxrwx    1 root     root            16 Jun 22 13:03 boot_b -> /dev/block/sda82

# lrwxrwxrwx    1 root     root            16 Jun 22 13:03 dtbo_a -> /dev/block/sda75
# lrwxrwxrwx    1 root     root            16 Jun 22 13:03 dtbo_b -> /dev/block/sda89

